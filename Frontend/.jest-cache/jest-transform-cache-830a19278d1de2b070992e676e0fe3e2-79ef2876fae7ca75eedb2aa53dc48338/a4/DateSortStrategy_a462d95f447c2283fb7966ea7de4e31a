"use strict";
Object.defineProperty(exports, "__esModule", { value: true });
exports.DateSortStrategy = void 0;
/**
 * Concrete strategy for sorting by date
 * Follows Single Responsibility Principle - only handles date sorting
 */
class DateSortStrategy {
    constructor(order) {
        Object.defineProperty(this, "order", {
            enumerable: true,
            configurable: true,
            writable: true,
            value: order
        });
    }
    sort(matches) {
        return [...matches].sort((a, b) => {
            const dateA = new Date(a.dateTime).getTime();
            const dateB = new Date(b.dateTime).getTime();
            const comparison = dateA - dateB;
            return this.order === 'desc' ? -comparison : comparison;
        });
    }
    getSortName() {
        return `Date ${this.order === 'desc' ? 'Descending' : 'Ascending'}`;
    }
}
exports.DateSortStrategy = DateSortStrategy;
