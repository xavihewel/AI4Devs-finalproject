{
  "name": "@bonareago/frontend",
  "version": "0.1.0",
  "description": "bon√ÄreaGo - SPA that consumes backend microservices. - OIDC Client: Keycloak (`frontend-spa`) with PKCE. - Env template: see `doc/setup/env.md` (VITE_* variables).",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "NODE_ENV=development npx jest --maxWorkers=50%",
    "test:watch": "NODE_ENV=development npx jest --watch --maxWorkers=50%",
    "test:coverage": "NODE_ENV=development npx jest --coverage --maxWorkers=50%",
    "test:fast": "NODE_ENV=development npx jest --maxWorkers=75% --silent",
    "test:ci": "NODE_ENV=development npx jest --maxWorkers=50% --ci --coverage --watchAll=false",
    "test:int": "RUN_INT=1 npx jest src/integration --runInBand",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "test:e2e": "cypress run --browser electron --headless",
    "test:e2e:open": "cypress open",
    "test:e2e:electron": "cypress run --browser electron --headless",
    "test:e2e:smoke": "cypress run --spec 'cypress/e2e/01-smoke/**/*' --browser electron --headless",
    "test:e2e:bypass": "CYPRESS_authDisabled=true cypress run --spec 'cypress/e2e/01-smoke/**/*' --browser electron --headless",
    "test:e2e:bypass:all": "CYPRESS_authDisabled=true cypress run --browser electron --headless --spec 'cypress/e2e/{01-smoke,03-trips,04-matches,05-bookings,06-flows}/**/*'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@googlemaps/react-wrapper": "^1.2.0",
    "@types/leaflet": "^1.9.20",
    "axios": "^1.12.2",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "keycloak-js": "^26.2.0",
    "leaflet": "^1.9.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^16.0.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^7.9.3"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.24.0",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cypress": "^0.1.6",
    "@types/i18next": "^12.1.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.7.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "axios-mock-adapter": "^2.1.0",
    "cypress": "^15.4.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.3",
    "vite": "^7.1.9"
  }
}
